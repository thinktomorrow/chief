{
    "name": "thinktomorrow/chief",
    "description": "Chief site management application",
    "keywords": ["site management", "laravel", "think tomorrow"],
    "license": "none",
    "type": "chief-cms",
    "require": {
        "php": "^7.1.3",
        "dimsav/laravel-translatable": "^9.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "~5.6.12|~5.7.1",
        "laravel/tinker": "^1.0",
        "spatie/laravel-activitylog": "^3.0",
        "spatie/laravel-image-optimizer": "^1.1",
        "spatie/laravel-permission": "^2.20",
        "spatie/image-optimizer": "1.1.4",
        "thinktomorrow/assetlibrary": "~0.5.0",
        "thinktomorrow/squanto": "^0.5.3",
        "thinktomorrow/vine": "~0.2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^4.0",
        "mockery/mockery": "^1.1",
        "nunomaduro/collision": "^2.0",
        "orchestra/testbench": "3.6.*|3.7.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Thinktomorrow\\Chief\\App\\" : "app/",
            "Thinktomorrow\\Chief\\" : "src/"
        },
        "files": [
            "src/Common/vendors/htmlLawed.php",
            "src/Common/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Thinktomorrow\\Chief\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thinktomorrow\\Chief\\App\\Providers\\ChiefServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
