    "vue": "^2.5.16",
    "es6-promise": "^4.1.1",
    "npmlog": "^4.1.2",
    "stickyfilljs": "^2.0.3",
    "vue-loader": "^14.2.2",
    "tippy.js": "^2.1.1",
    "vue-multiselect": "^2.0.3",
    "warpaint": "https://github.com/thinktomorrow/warpaint.git#0.1"
  }
